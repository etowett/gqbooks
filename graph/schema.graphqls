# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Book {
  title: String!
  author: String!
  pages: [Page!]!
}

type Page {
  pageIndex: Int!
  content: String!
  refined_tokens: [Combined!]!
  tokens: [Token!]!
}

type Combined  {
	index: Int
	token: String
	content: String!
	isTappable: Boolean!
}

type Token {
  position: [Int!]!
  value: String!
}

type Query {
  books: [Book!]!
}
